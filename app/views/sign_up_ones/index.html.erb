<p id="notice"><%= notice %></p>

<h1>Listing Sign Up Ones</h1>

<table border="5">
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Middlename</th>
      <th>Lastname</th>
      <th>Username</th>
      <th>Email</th>
      <th>Phoneno</th>
      <th>Cellno</th>
      <th>City</th>
      <th>Dateofbirth</th>
      <th>Password</th>
      <th>Confirmpassword</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sign_up_ones.each do |sign_up_one| %>
      <tr>
        <td><%= sign_up_one.firstname %></td>
        <td><%= sign_up_one.middlename %></td>
        <td><%= sign_up_one.lastname %></td>
        <td><%= sign_up_one.username %></td>
        <td><%= sign_up_one.email %></td>
        <td><%= sign_up_one.phoneno %></td>
        <td><%= sign_up_one.cellno %></td>
        <td><%= sign_up_one.city %></td>
        <td><%= sign_up_one.dateofbirth %></td>
        <td><%= sign_up_one.password %></td>
        <td><%= sign_up_one.confirmpassword %></td>
        <td><%= link_to 'Show', sign_up_one %></td>
        <td><%= link_to 'Edit', edit_sign_up_one_path(sign_up_one) %></td>
        <td><%= link_to 'Destroy', sign_up_one, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

